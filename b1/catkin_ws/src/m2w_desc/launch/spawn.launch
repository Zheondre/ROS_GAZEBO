<?xml version="1.0"?>
<launch>

  <param name="robot_description" command="$(find xacro)xacro.py '$(find m2w_desc)/urdf/m2w.xacro'"/>

  <arg name="x" default="0"/>
  <arg name="y" default="0"/>
  <arg name="z" default="0.5"/>

 <node name="m2w_spawn" pkg="gazebo_ros" type="spawn_model" output ="screen" 
      args="-urdf -param robot_description -model m2w -x $(arg x) -y $(arg y) -z $(arg z)" 

/>

</launch>
